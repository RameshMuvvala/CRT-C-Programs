#include<stdio.h>
int duplicate(int arr[],int n)
{
	int count = 0;
	for(int i = 0;i<n;i++)
	{
		for(int j = i+1;j<n;j++)
		{
			if(arr[i]==arr[j])
			{
				count++;
				break;
			}
		}
	}
	return count;
}
int main()
{
	int arr[] = {1,1,2,2,3,4,5,5};
	int n = sizeof(arr)/sizeof(arr[0]);
	int d = duplicate(arr,n);
	printf("No of duplicate elements: %d",d);
	return 0;	
}
